{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId"
    ],
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": { "expression": "@mt = 'An unhandled exception has occurred while executing the request.'" }
      }
    ],
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": { "serverUrl": "https://seqlogger.azurewebsites.net/" }
      }
    ],
    "Properties": {
      "ApplicationName": "SalesApp.Web"
    }
  },
  "AllowedHosts": "*"
}